package com.example.demo.pojo;

import javax.persistence.Entity;

@Entity
public class User {
	private Integer id;
	private String name;
	
	public void setId(Integer id) {
		this.id = id;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public Integer getId() {
		return this.id;
	}
	
	public String getName() {
		return this.name;
	}
}
